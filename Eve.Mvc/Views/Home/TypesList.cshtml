@model Eve.Mvc.Models.TypesListViewModel
@* @{
    ViewData["Title"] = "Home Page";
} *@

<table class="table">
    <thead>
        <tr>
            <td style="text-align: left;">Name</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var type in Model.eveUniverseTypes)
        {
            <tr>
                <td style="text-align: left;">
                    <a href="#" class="typeLink" data-id="@type.TypeId">@type.Name</a>
                </td>
            </tr>
        }
    </tbody>
</table>
